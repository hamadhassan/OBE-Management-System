<Window x:Class="Application_CLOs.StudentResultDashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Application_CLOs"
        mc:Ignorable="d"
        Title="StudentResultDashboard" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="409*"/>
            <RowDefinition Height="25*"/>
        </Grid.RowDefinitions>
        <Button x:Name="btnMarkResult" Content="Mark Result" HorizontalAlignment="Left" Margin="200,179,0,0" VerticalAlignment="Top" Click="btnMarkResult_Click"/>
        <Button Content="View Student Result" HorizontalAlignment="Left" Margin="325,189,0,0" VerticalAlignment="Top" Click="Button_Click"/>

    </Grid>
</Window>
